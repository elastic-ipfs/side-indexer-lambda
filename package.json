{
  "name": "side-indexer-lambda",
  "version": "0.0.0",
  "description": "AWS Lambda that creates CARv2 side indexes for CAR files uploaded to an S3 bucket.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "ava --verbose",
    "lint": "standard",
    "deploy": "aws lambda update-function-code --cli-connect-timeout 15000 --region $AWS_REGION --function-name $LAMBDA --image $AWS_ACCOUNT_ID.dkr.ecr.$AWS_REGION.amazonaws.com/$AWS_ECR_REPOSITORY:$DOCKER_IMAGE_VERSION"
  },
  "author": "Alan Shaw",
  "license": "Apache-2.0 OR MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.100.0",
    "@ipld/car": "^4.1.3",
    "cardex": "^0.0.0",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.97",
    "ava": "^4.3.0",
    "multiformats": "^9.6.5",
    "standard": "^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs-elastic-provider/side-indexer-lambda.git"
  },
  "keywords": [
    "index",
    "indexer",
    "CAR",
    "CARv2",
    "lambda",
    "aws"
  ],
  "bugs": {
    "url": "https://github.com/ipfs-elastic-provider/side-indexer-lambda/issues"
  },
  "homepage": "https://github.com/ipfs-elastic-provider/side-indexer-lambda#readme"
}
